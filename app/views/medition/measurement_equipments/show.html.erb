<div class="row"><br></div>
<div class="row"><br></div>
<div class="container-fluid">
  <%= link_to '<button class="btn btn-default btn-sm">
  <i class="fa fa-arrow-left" aria-hidden="true"></i> Regresar</button>'.html_safe,medition_measurement_equipments_path %>
</div>
<div class="row"><br></div>
<div class="container-fluid">
  <div class="panel panel-success">
    <div class="panel-heading"><h3>Detalles del equipo de medición <span class="label label-default">  <%= @measurement_equipment.modelo %></span></h3></div>
    <div class="panel-body">
      <div class="row"><br></div>
      <div class="row"><br></div>
      <div class="row">
       <div class="col-xs-6 col-md-3">
         <p>
          <strong>Tipo de  Equipo medición :</strong>
          <%= @measurement_equipment.equipo_medicion_type %>
         </p>
        </div>
        <div class="col-xs-6 col-md-3">
         <p>
          <strong>Modelo:</strong>
          <%= @measurement_equipment.modelo %>
         </p>
        </div>
        <div class="col-xs-6 col-md-3">
          <p>
            <strong>Marca:</strong>
            <%= @measurement_equipment.marca %>
          </p>
        </div>
     </div>
     <div class="row"><br></div>
     <div class="row">
       <div class="col-xs-6 col-md-3">
         <p>
          <strong>Número de serie:</strong>
          <%= @measurement_equipment.num_serie %>
         </p>
        </div>
        <div class="col-xs-6 col-md-3">
         <p>
          <strong>Num inv eimp:</strong>
          <%= @measurement_equipment.num_inv_eimp %>
         </p>
        </div>
     </div>
     <div class="row"><br></div>
     <div class="row"><br></div>
     <div class="row show-grid">
        <div class="col-md-3"></div>
        <div class="col-md-3 col-md-offset-5 float_right">
            <div class="btn-toolbar" role="toolbar" aria-label="...">
               <div class="btn-group" role="group" aria-label="...">
                  <%= link_to '<button type="submit" class="btn btn-default" ><i class="fa fa-pencil" aria-hidden="true"></i>   Editar calibración</button>'.html_safe, edit_medition_measurement_equipment_path(@measurement_equipment)%>
               </div>
            </div>
        </div>
      </div>
    </div>
  </div>
</div>
