
         <%= simple_form_for(['medition',@calibration]) do |f| %>
          <%= f.error_notification %>
          <div class="row"><br></div>
          <div class="row"><br></div>
          <div class="row">
             <div class="col-xs-6 col-md-4">
               <div class="form-group">  
               <%= f.input :fecha_calibracion,label: 'Fecha de calibración',class_html: { class: 'form-control' }  %>
                </div>
             </div>
             <div class="col-xs-6 col-md-4">
               <div class="form-group">  
                <%= f.input :fecha_vencimiento ,label: 'Fecha de vencimiento',class_html: { class: 'form-control' }  %>
                </div>
             </div>
          </div>
          <div class="row"><br></div>
          <div class="row">
             <div class="col-xs-6 col-md-4">
               <div class="form-group">  
                 <%= f.label :Modelo_de_equipo_de_medición ,class:"col-sm-10 control-label" %>
                <div class="col-sm-10">
                   <%= f.select :measurement_equipment_id , MeasurementEquipment.all.map { |u| [u.num_serie, u.id ]}, { prompt: '- Seleccionar -' },{class:"selectpicker show-tick"} %>
                </div>
              </div>
             </div>
          </div>
          <div class="row"><br></div>
          <div class="row"><br></div>
          <div class="row"><br></div>
          <div class="row show-grid">
            <div class="col-md-3"></div>
            <div class="col-md-4 col-md-offset-5 float_right">
                <div class="btn-toolbar" role="toolbar" aria-label="...">
                   <div class="btn-group" role="group" aria-label="...">
                      <button  class="btn btn-default" type="submit" >  
                          <i class="fa fa-check" aria-hidden="true"></i> Crear calibración
                      </button>
                   </div>
                   <div class="btn-group" role="group" aria-label="...">
                      <%= link_to '<button type="submit" class="btn btn-default" ><i class="fa fa-ban" aria-hidden="true"></i> Cancelar
                      </button>'.html_safe,medition_calibrations_path %>
                      <% end %>
                   </div>
                </div>
            </div>
          </div>
      </div>
  </div>
</div>
      


