<div class="row"><br></div>
<div class="row"><br></div>
<div class="container-fluid">
  <%= link_to '<button class="btn btn-default btn-sm">
  <i class="fa fa-arrow-left" aria-hidden="true"></i> Regresar</button>'.html_safe, equipment_transformers_path  %>
</div>
<div class="row"><br></div>
<div class="container-fluid">
  <div class="panel panel-success">
    <div class="panel-heading"><h3>Detalles del Transformador <span class="label label-default">  <%= @transformer.nomenclatura %></span></h3></div>
    <div class="panel-body">
      <div class="row"><br></div>
      <div class="row"><br></div>
      <div class="row">
       <div class="col-xs-6 col-md-3">
         <p>
          <strong>Nomenclatura:</strong>
          <%= @transformer.nomenclatura %>
         </p>
        </div>
        <div class="col-xs-6 col-md-3">
         <p>
          <strong>Tipo de transformador:</strong>
          <%= @transformer.transformador_type %>
         </p>
        </div>
        <div class="col-xs-6 col-md-3">
          <p>
            <strong>Tipo:</strong>
            <%= @transformer.phase_type %>
          </p>
        </div>
        <div class="col-xs-6 col-md-3">
         <p>
          <strong>Numero activo fijo:</strong>
          <%= @transformer.numero_activo_fijo %>
         </p>
        </div>
     </div>
      <div class="row">
       <div class="col-xs-6 col-md-3">
        <p>
          <strong>Fecha puesta servicio:</strong>
          <%= @transformer.fecha_puesta_servicio %>
        </p>
       </div>
      </div>
      <div class="row">
        <div class="col-xs-6 col-md-3">
            <h1 class="page-header">Fases </h1>
        </div>
      </div>
      <%  @transformer.phases.each do |t| %>
      <div class="row">
        <div class="col-xs-6 col-md-3">
            <h4>Fase: <%= t.fase %></h4> 
        </div>
      </div>
      <div class="row"><br></div>
      <div class="row">
        <div class="col-xs-6 col-md-3">
            <label>Marca: </label> <%= t.marca %> 
        </div>
        <div class="col-xs-6 col-md-3">
           <label>Numero de serie: </label> <%= t.num_serie %>
        </div>
        <div class="col-xs-6 col-md-3">
           <label>Fecha de fabricación: </label> <%= t.fecha_fabricacion %>  
        </div>  
      </div>
      <div class="row"><br></div>
      <div class="row">
        <div class="col-xs-6 col-md-3">
            <label>Fecha puesta en servicio: </label><%= t.fecha_puesta_servicio %>  
        </div>
        <div class="col-xs-6 col-md-3">
            <label>Masa de embarque: </label> <%= t.masa_embarque %>
        </div>
        <div class="col-xs-6 col-md-3">
           <label>Esquema de núcleo: </label> <%= t.esquema_nucleo %>
        </div>
      </div>
      <div class="row"><br></div>
      <div class="row">
        <div class="col-xs-6 col-md-3">
            <label>Total de aceite: </label><%= t.total_aceite %> 
        </div>
        <div class="col-xs-6 col-md-3">
            <label>Sistema de enfriamiento: </label> <%= t.sistema_enfriamiento %>
        </div>
        <div class="col-xs-6 col-md-3">
            <label>Mampara: </label>  <%= t.mampara %>
        </div>
      </div>
      <div class="row"><br></div>
      <div class="row">
        <div class="col-xs-6 col-md-3">
            <label>Numero taps: </label><%= t.num_taps %>
        </div>
        <div class="col-xs-6 col-md-3">
            <label>Sistema contra incendios: </label> <%= t.sistema_contra_incendios %>
        </div>
        <div class="col-xs-6 col-md-3">
            <label>Fosa captadora de aceite: </label>  <%= t.fosa_captadora_aceite %>
        </div>
        <div class="col-xs-6 col-md-3">
           <label>Tension de sistema: </label> <%= t.tension_sistema %>
        </div>
      </div>
      <div class="row"><br></div>
      <div class="row">
        <div class="col-xs-6 col-md-3">
            <label>Clase de exactitud: </label> <%= t.clase_exactitud %>
        </div>
        <div class="col-xs-6 col-md-3">
            <label>Tipo de expanción de aceite: </label> <%= t.tipo_expancion_aceite %> 
        </div>
        <div class="col-xs-6 col-md-3">
           <label>Tipo envolvente: </label><%= t.tipo_envolvente %>
        </div>
      </div>
      <div class="row"><br></div>
      <div class="row"><br></div>
      <div class="row"><br></div>
      <% end %>
      <div class="row">
        <div class="col-xs-6 col-md-3">
            <h1 class="page-header">Boquillas</h1>
        </div>
      </div>
      <%  @transformer.trans_mouthpieces.each do |t| %>
      <div class="row">
        <div class="col-xs-6 col-md-3">
            <label>Marca: </label><%= t.marca %>
        </div>
        <div class="col-xs-6 col-md-3">
            <label>Tipo: </label> <%= t.tipo %>
        </div>
        <div class="col-xs-6 col-md-3">
            <label>Numero de serie: </label> <%= t.num_serie %> 
        </div>
        <div class="col-xs-6 col-md-3">
           <label>Kv: </label> <%= t.kv %> 
        </div>
      </div>
      <div class="row"><br></div>
      <div class="row">
        <div class="col-xs-6 col-md-3">
            <label>Amp: </label> <%= t.amp %>
        </div>
        <div class="col-xs-6 col-md-3">
            <label>Fecha de fabricación: </label> <%= t.fecha_fabricacion %> 
        </div>
      </div>
      <div class="row"><br></div>
      <div class="row">
        <div class="col-xs-6 col-md-3">
            <h1 class="page-header">Cambiador de taps</h1>
        </div>
      </div>
      <% end %>
      <div class="row"><br></div>
      <div class="row"><br></div>
      <%  @transformer.tap_changers.each do |t| %>
      <div class="row">
        <div class="col-xs-6 col-md-3">
            <label>Marca: </label><%= t.marca %>
        </div>
        <div class="col-xs-6 col-md-3">
            <label>Tipo: </label> <%= t.tipo %>
        </div>
        <div class="col-xs-6 col-md-3">
            <label>Numero de serie: </label> <%= t.serie %> 
        </div>
        <div class="col-xs-6 col-md-3">
           <label>Numero de pasos: </label> <%= t.num_pasos %> 
        </div>
      </div>
      <div class="row"><br></div>
      <div class="row"><br></div>
      <div class="row"><br></div>
      <div class="row"><br></div>
      <% end %>
      <div class="row show-grid">
        <div class="col-md-3"></div>
        <div class="col-md-3 col-md-offset-5 float_right">
            <div class="btn-toolbar" role="toolbar" aria-label="...">
               <div class="btn-group" role="group" aria-label="...">
                  <%= link_to '<button type="submit" class="btn btn-default" ><i class="fa fa-pencil" aria-hidden="true"></i>   Editar Transformador</button>'.html_safe, edit_equipment_transformer_path(@transformer) %>
               </div>
            </div>
        </div>
      </div>
    </div>
  </div>
</div>
